<schema
  xmlns='http://www.w3.org/2001/XMLSchema'
  targetNamespace='http://java.sun.com/dtd/web-app_2_3.dtd'
  xmlns:wa='http://java.sun.com/dtd/web-app_2_3.dtd'>

 <element name='web-app'>
  <complexType>
   <sequence>
    <element ref='wa:icon' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:display-name' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:distributable' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:context-param' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:filter' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:filter-mapping' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:listener' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:servlet' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:servlet-mapping' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:session-config' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:mime-mapping' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:welcome-file-list' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:error-page' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:taglib' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:resource-env-ref' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:resource-ref' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:security-constraint' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:login-config' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:security-role' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:env-entry' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:ejb-ref' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:ejb-local-ref' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='auth-constraint'>
  <complexType>
   <sequence>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:role-name' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='auth-method'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='context-param'>
  <complexType>
   <sequence>
    <element ref='wa:param-name'/>
    <element ref='wa:param-value'/>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='description'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='display-name'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='distributable'>
  <complexType>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='ejb-link'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='ejb-local-ref'>
  <complexType>
   <sequence>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:ejb-ref-name'/>
    <element ref='wa:ejb-ref-type'/>
    <element ref='wa:local-home'/>
    <element ref='wa:local'/>
    <element ref='wa:ejb-link' minOccurs='0' maxOccurs='1'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='ejb-ref'>
  <complexType>
   <sequence>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:ejb-ref-name'/>
    <element ref='wa:ejb-ref-type'/>
    <element ref='wa:home'/>
    <element ref='wa:remote'/>
    <element ref='wa:ejb-link' minOccurs='0' maxOccurs='1'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='ejb-ref-name'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='ejb-ref-type'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='env-entry'>
  <complexType>
   <sequence>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:env-entry-name'/>
    <element ref='wa:env-entry-value' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:env-entry-type'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='env-entry-name'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='env-entry-type'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='env-entry-value'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='error-code'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='error-page'>
  <complexType>
   <sequence>
    <choice>
     <element ref='wa:error-code'/>
     <element ref='wa:exception-type'/>
    </choice>
    <element ref='wa:location'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='exception-type'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='extension'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='filter'>
  <complexType>
   <sequence>
    <element ref='wa:icon' minOccurs='0' maxOccurs='1'/>
