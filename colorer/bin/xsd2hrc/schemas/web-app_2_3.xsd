   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='session-timeout'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='small-icon'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='taglib'>
  <complexType>
   <sequence>
    <element ref='wa:taglib-uri'/>
    <element ref='wa:taglib-location'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='taglib-location'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='taglib-uri'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='transport-guarantee'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='url-pattern'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='user-data-constraint'>
  <complexType>
   <sequence>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:transport-guarantee'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='web-resource-collection'>
  <complexType>
   <sequence>
    <element ref='wa:web-resource-name'/>
    <element ref='wa:description' minOccurs='0' maxOccurs='1'/>
    <element ref='wa:url-pattern' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='wa:http-method' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='web-resource-name'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='welcome-file'>
  <complexType mixed='true'>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>

 <element name='welcome-file-list'>
  <complexType>
   <sequence>
    <element ref='wa:welcome-file' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
  </complexType>
 </element>
</schema>
