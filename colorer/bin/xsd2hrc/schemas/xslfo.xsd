      <enumeration value='center'/>
      <enumeration value='end'/>
      <enumeration value='justify'/>
      <enumeration value='inside'/>
      <enumeration value='outside'/>
      <enumeration value='left'/>
      <enumeration value='right'/>
      <enumeration value='inherit'/>
     </restriction>
    </simpleType>
   </attribute>
  </attributeGroup>

  <group name='basic-inlines'>
<!-- fo:bidi-override  | fo:character  | fo:external-graphic  | fo:instream-foreign-object   | fo:inline  | fo:inline-container  | fo:leader  | fo:page-number  | fo:page-number-citation  | fo:basic-link  | fo:multi-toggle  | rx:pinpoint  | rx:page-index -->
   <choice>
    <element ref='fo:bidi-override'/>
    <element ref='fo:character'/>
    <element ref='fo:external-graphic'/>
    <element ref='fo:instream-foreign-object'/>
    <element ref='fo:inline'/>
    <element ref='fo:inline-container'/>
    <element ref='fo:leader'/>
    <element ref='fo:page-number'/>
    <element ref='fo:page-number-citation'/>
    <element ref='fo:basic-link'/>
    <element ref='fo:multi-toggle'/>
   </choice>
  </group>

  <group name='basic-blocks'>
<!-- fo:block  | fo:block-container  | fo:table-and-caption  | fo:table  | fo:list-block -->
   <choice>
    <element ref='fo:block'/>
    <element ref='fo:block-container'/>
    <element ref='fo:table-and-caption'/>
    <element ref='fo:table'/>
    <element ref='fo:list-block'/>
   </choice>
  </group>

  <group name='out-of-lines'>
<!-- fo:footnote  | fo:float -->
   <choice>
    <element ref='fo:footnote'/>
    <element ref='fo:float'/>
   </choice>
  </group>

  <group name='wrappers'>
<!-- fo:multi-switch  | fo:multi-properties  | fo:wrapper  | fo:marker  | fo:retrieve-marker -->
   <choice>
    <element ref='fo:multi-switch'/>
    <element ref='fo:multi-properties'/>
    <element ref='fo:wrapper'/>
    <element ref='fo:marker'/>
    <element ref='fo:retrieve-marker'/>
   </choice>
  </group>

  <group name='inlines'>
<!-- #PCDATA  | #MODELGROUPREF basic-inlines  | #MODELGROUPREF out-of-lines  | #MODELGROUPREF wrappers -->
   <choice>
    <group ref='fo:basic-inlines'/>
    <group ref='fo:out-of-lines'/>
    <group ref='fo:wrappers'/>
   </choice>
  </group>

  <group name='blocks'>
<!-- #MODELGROUPREF basic-blocks  | #MODELGROUPREF out-of-lines  | #MODELGROUPREF wrappers -->
   <choice>
    <group ref='fo:basic-blocks'/>
    <group ref='fo:out-of-lines'/>
    <group ref='fo:wrappers'/>
   </choice>
  </group>

  <group name='mixed-level'>
<!-- #PCDATA  | #MODELGROUPREF basic-inlines  | #MODELGROUPREF basic-blocks  | #MODELGROUPREF out-of-lines  | #MODELGROUPREF wrappers -->
   <choice>
    <group ref='fo:basic-inlines'/>
    <group ref='fo:basic-blocks'/>
    <group ref='fo:out-of-lines'/>
    <group ref='fo:wrappers'/>
   </choice>
  </group>

  <group name='row-group'>
<!-- fo:table-row+ | fo:table-cell+  -->
   <choice>
    <element ref='fo:table-row' maxOccurs='unbounded'/>
    <element ref='fo:table-cell' maxOccurs='unbounded'/>
   </choice>
  </group>

