      <enumeration value='uniform'/>
      <enumeration value='non-uniform'/>
      <enumeration value='inherit'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='scaling-method' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='auto'/>
      <enumeration value='integer-pixels'/>
